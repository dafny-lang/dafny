  assert a << 2 == 12;
  assert a >> 0 == 3;
  assert a >> 1 == 1;
  assert a >> 2 == 0;
  var b: bv5 := 24;
  assert b << 1 == 16;
  assert b >> 0 == 24;
  assert b >> 1 == 12;
  assert b >> 2 == 6;
}

