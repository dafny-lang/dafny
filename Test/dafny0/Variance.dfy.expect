module Cycles {
  type W0 = W1

  datatype W1 = Ctor(W0 --> bool)

  datatype Z0 = Ctor(Z2 ~> bool)

  type Z1 = Z0

  datatype Z2 = Ctor(Z1)
}

module Depen {
  type V0 = x: V2
    | true

  type V1 = V0

  datatype V2 = Ctor(V1 --> bool)
}

module DependencyChecks {
  type A = x: B
    | true

  type B = y: int
    | F(y)

  datatype Q = Q(x: R)

  type R = x: S
    | true

  type S = y: int
    | G(y)

  ghost predicate F(y: int)
  {
    var i: A := 5;
    i < 32
  }

  ghost predicate G(y: int)
  {
    var q: Q :| true;
    q.x < 32
  }
}

module Cycle0 {
  type B = x: B
    | true
}

module Cycle1 {
  type A = x: int
    | var a: A :| true; P(x)

  ghost predicate P(x: int)
}

module Cycle2 {
  type C = x: int
    | f(x) == f(x)

  ghost function f(x: int): C
}

